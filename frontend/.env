# WEB application development service port
WEB_DEV_PORT=9000
# Interface Origin address
WEB_API_ORIGIN=/
# WebSocket Host address
WEB_WS_HOST=/
# Interface API proxy address
WEB_API_PROXY=https://demo.ariot.com.tr/api/v1
# WebSocket proxy address
WEB_SOCKET_PROXY=wss://demo.ariot.com.tr
