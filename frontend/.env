# MOCK_API_PROXY=https://api.escuelajs.co

# Enable HTTPS for development
# ENABLE_HTTPS=false

# Enable VConsole
ENABLE_VCONSOLE=true

# Enable Service Worker
ENABLE_SW=false

# Enable Service Worker in development mode
